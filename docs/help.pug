extends _docs.tpl.jade

block append vars
  - var page = Docs.DOCS.find('/docs/help.html')

block append main
  +newSection(2, Docs.DOCS.find('/docs/help.html#block'))
    include _includes/_help.block.pug
  +newSection(2, Docs.DOCS.find('/docs/help.html#inline'))
    include _includes/_help.inline.pug
  +newSection(2, Docs.DOCS.find('/docs/help.html#clearfix'))
    include _includes/_help.clearfix.pug
  +newSection(2, Docs.DOCS.find('/docs/help.html#hidden'))
    include _includes/_help.hidden.pug
