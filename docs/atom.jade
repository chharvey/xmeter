extends _docs.tpl.jade

block append vars
  - var page = Xmeter.DOCS.find('/docs/atom.html')

block append main
  +newSection(2, Xmeter.DOCS.find('/docs/atom.html#margin-bottom'))
    include _includes/_atom.margin-bottom.jade
  +newSection(2, Xmeter.DOCS.find('/docs/atom.html#padding-top'))
    include _includes/_atom.padding-top.jade
  +newSection(2, Xmeter.DOCS.find('/docs/atom.html#font-size'))
    include _includes/_atom.font-size.jade
